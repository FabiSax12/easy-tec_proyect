{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "pnpm install",
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist",
        "maxDuration": 20
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ],
  "build": {
    "env": {
      "VERCEL_NATIVE_BUILD": "1"
    }
  }
}